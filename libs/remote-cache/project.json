{
  "name": "remote-cache",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/remote-cache/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/remote-cache",
        "main": "libs/remote-cache/src/index.ts",
        "tsConfig": "libs/remote-cache/tsconfig.lib.json",
        "assets": ["libs/remote-cache/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "libs/remote-cache/**/*.ts",
          "libs/remote-cache/package.json"
        ]
      }
    }
  },
  "tags": []
}
